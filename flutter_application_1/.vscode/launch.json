    "version": "0.2.0",
    "configurations": [

        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        
        {
            "name": "flutter_application_1",
            "request": "launch",
            "type": "dart",
            "console": "terminal",
            "args": "[-- no-sound-null-safety-BROKEN]",
             
            },
        {
            "name": "flutter_application_1 (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_application_1 (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}